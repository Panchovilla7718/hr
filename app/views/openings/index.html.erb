<h1>Vacancies</h1>

</hr>
    <div class = "row center">
        <table class="table table-striped" >
        <thead></thead>
            <tr>
                <th>id</th>
                <th>position</th>
                <th>department</th>
                <th>schedule</th>
                <th>supervisor</th>
                <th>created</th>
            </tr>
        <thead/>
        <tbody>
        <% @openings.each do |t| %>
            <td><%=link_to t.id, opening_path(t) %></td>
            <td><%=t.position %></td>
            <td><%=t.dept %></td>
            <td><%=t.schedule %></td>
            <td><%=t.supervisor.name%></td>
            <td><%=t.created_at.strftime("%b %d %Y")%></td>
         </tbody> 
        <% end %>
        
        </table>
    </div>
