<h1>Vacancies</h1>

</hr>
<% @openings.each do |t| %>
    <div class = "row center">
        <table class="table table-striped">
            <tr>
                <th>id</th>
                <th>position</th>
                <th>department</th>
                <th>schedule</th>
                <th>supervisor</th>
                <th>created</th>
            </tr>
            <td><%= t.id%></td>
            <td><%=t.position %></td>
            <td><%=t.dept %></td>
            <td><%=t.schedule %></td>
            <td><%=t.supervisor.name%></td>
            <td><%=t.created_at.strftime("%b %d %Y")%></td>
        </table>
    </div>
<% end %>